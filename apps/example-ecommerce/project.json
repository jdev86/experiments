{
  "root": "apps/example-ecommerce",
  "sourceRoot": "apps/example-ecommerce/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nxext/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/example-ecommerce",
        "frameworkConfigFile": "@nxext/solid/plugins/vite"
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "@nxext/vite:dev",
      "options": {
        "outputPath": "dist/apps/example-ecommerce",
        "frameworkConfigFile": "@nxext/solid/plugins/vite"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/example-ecommerce/**/*.{ts,spec.ts,tsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/example-ecommerce"],
      "options": {
        "jestConfig": "apps/example-ecommerce/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
